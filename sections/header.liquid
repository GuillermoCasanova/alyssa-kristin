    }
}

.header__icon {
    width: 2.5rem;
    height: 2.5rem;
    position: relative;
    background-color: transparent;
    border: none;
    color: rgb(var(--color-base-text-1));
    display: block;
}

.header__icon:hover {
    cursor: pointer;
}

.header__icon .icon {
    fill: var(--color-base-text);
    vertical-align: middle;
}

.header__icon svg {
    height: 100%;
    width: 100%;
}


.header__icon--search {
    display: none;
}

@media screen and (min-width: 989px) {
    .header__icon--search {
        display: block;
    }
