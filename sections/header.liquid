/*------------------------------------*\
    #Header Heading
\*------------------------------------*/


.header__heading {
    grid-area: heading;
    justify-self: center;
    line-height: 0;
    margin: 0;
    z-index: 2;
    width: auto; 
}

.header__heading-link  {
    display: block;
    width: 100%; 
}

@media screen and (min-width: 940px) {
    .header__heading {
        position: absolute;
    }
}


.header__logo {
    fill: rgb(var(--color-base-text-1));
    height: 4rem;
    margin-left: auto;
    margin-right: auto;
    max-width: 19rem;
    transition: all var(--duration-default) var(--default-easing);
    z-index: 0;
    width: 47vw;
    min-width: 16rem;
    margin-top: 1rem; 
}


@media screen and (min-width: 1100px) {
    .header__logo {
        max-width: 22rem;
    }
}

@media screen and (min-width: 1280px) {
    .header__logo {
        max-width: 22rem;
    }
}


.header__logo svg {
    height: 100%; 
    width: 100%; 
}

.menu-open .header__logo {
    fill: rgb(var(--color-base-text-2));
}

@media screen and (min-width: 940px) {
    .header__logo {
        position: relative;
    }

}


.header__localization {
    display: none;
}





/*------------------------------------*\
    #Header Drawer Blocker
\*------------------------------------*/

.header-drawer-blocker {
    background: rgb(var(--color-base-background-1));
    border-bottom: 1px solid rgb(var(--color-base-text-2));
    bottom: 0;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    transition: all .2s ease-in-out;
    width: 100%;
    z-index: 1;
}

.menu-open .header-drawer-blocker {
    background: rgb(var(--color-base-background-2));
}

@media screen and (min-width: 940px) {
    header-drawer {
        display: none;
    }
}





/*------------------------------------*\
    #Header Icons
\*------------------------------------*/

.header__icons {
    display: flex;
    justify-content: flex-end;
    position: relative;
    padding-right: 0; 
}

@media screen and (min-width: 940px) {
    .header__icons {
        position: absolute;
        right: 0;
        top: 0; 
        position: absolute;
        right: var(--level4);
        top: .75rem;
        color: rgb(var(--color-base-text-1));
        margin-left: auto;
        margin-right: auto;
        left: auto;
        right: auto;
        padding-right: var(--level3);
